{
    "name": "area17/component-transformers",
    "description": "This is my package component-transformers",
    "keywords": [
        "area17",
        "laravel",
        "component-transformers"
    ],
    "homepage": "https://github.com/area17/component-transformers",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Brook",
            "email": "tim@area17.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^7.0",
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "A17\\ComponentTransformers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "A17\\ComponentTransformers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "A17\\ComponentTransformers\\ComponentTransformersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
